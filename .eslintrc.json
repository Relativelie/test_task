{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "no-plusplus": "off",
        "class-methods-use-this": "off",
        "import/extensions": "off",
        "array-callback-return": "off"
    }
}
